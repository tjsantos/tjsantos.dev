    "eslint-plugin-storybook": "^9.0.9",
    "jsdom": "^26.0.0",
    "playwright": "^1.53.0",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "storybook": "^9.0.9",
    "typescript": "^5.7.2",
    "vite": "^6.1.0",
    "vitest": "^3.0.5",
    "web-vitals": "^4.2.4",
    "wrangler": "^4.16.1"
  }
